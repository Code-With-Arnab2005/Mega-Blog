{
    "version": 2,
    "builds": [
      {
        "src": "build/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "build/static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/login",
        "dest": "/index.html"
      },
      {
        "src": "/signup",
        "dest": "/index.html"
      },
      {
        "src": "/all-posts",
        "dest": "/index.html"
      },
      {
        "src": "/add-post",
        "dest": "/index.html"
      },
      {
        "src": "/edit-post/:slug",
        "dest": "/index.html"
      },
      {
        "src": "/post/:slug",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }